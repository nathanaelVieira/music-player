<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>

<fx:root type="Group" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Button layoutX="2.0" layoutY="-1.0" mnemonicParsing="false"
			prefHeight="15.0" prefWidth="50.0" styleClass="next-button"
			stylesheets="@style-buttons.css">
			<effect>
				<InnerShadow>
					<input>
						<DropShadow />
					</input>
				</InnerShadow>
			</effect>
		</Button>
	</children>
</fx:root>
